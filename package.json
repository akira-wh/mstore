{
  "name": "MSTORE",
  "version": "1.0.0",
  "private": true,
  "description": "Clothing store",
  "author": "Akira",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.1.x",
    "browser-sync": "2.18.x",
    "del": "3.0.x",
    "gulp": "3.9.x",
    "gulp-csso": "3.0.x",
    "gulp-imagemin": "3.4.x",
    "gulp-plumber": "1.1.x",
