    "gulp-rename": "1.2.x",
    "gulp-sass": "3.1.x",
    "gulp-svgmin": "1.2.x",
    "gulp-svgstore": "6.1.x",
    "gulp-uglify": "3.0.x",
    "gulp-webp": "2.3.x",
    "run-sequence": "2.2.x",
